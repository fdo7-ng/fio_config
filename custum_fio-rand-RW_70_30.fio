; fio-rand-RW.job for fiotest

[global]
name=fio-rand-RW
filename=fio-rand-RW
rw=randrw
rwmixread=70
rwmixwrite=30
bs=4K
direct=0
numjobs=8
time_based=1
runtime=1800
size=200G
ioengine=libaio
iodepth=16


[job1]
rw=randread
filename=/fio
name=raw=random-read
[job2]
rw=randread
filename=/fio1
name=raw=random-read

[job3]
rw=randread
filename=/fio
name=raw=random-read
[job4]
rw=randread
filename=/fio1
name=raw=random-read

